# PHP artisan 
alias a="php artisan"
alias artisan="php artisan"
alias tinker="artisan tinker"
alias mfs="artisan migrate:fresh --seed"
alias p="artisan test"
alias pf="artisan test --filter"

# Phpunit watcher
alias wp="phpunit-watcher watch --stop-on-failure"
alias wpf="phpunit-watcher watch --stop-on-failure --filter"
