[color]
  	ui = true
        diff = auto
        status = auto
        branch = auto

[color "branch"]
  	current = yellow reverse
  	local = yellow
  	remote = green

[color "diff"]
  	frag = magenta bold
  	meta = yellow
  	new = green bold
  	old = red bold

[color "status"]
  	added = yellow
  	changed = green
  	deleted = red
  	untracked = cyan

