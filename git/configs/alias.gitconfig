[alias]
	amend = commit --amend
  	changes = log --name-status --pretty=format:'%C(bold blue)%h%C(reset) - %C(green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)'
  	gl = log --graph --abbrev-commit --decorate --date=relative --format=format:'%C(bold blue)%h%C(reset) - %C(green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)'
        new = checkout -b
  	staged = diff --cached
  	uncommit = reset --soft HEAD^
  	unstage = reset HEAD
  	unstaged = diff
